{
  "name": "tinywan/nacos-sdk-php",
  "description": "php sdk for nacos",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Tinywan",
      "email": "756684177@qq.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ext-openssl": "*",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "^6.3 || ^7.0",
    "monolog/monolog": "^1.24|^2.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5|^8.0"
  },
  "autoload": {
    "psr-4": {
      "nacos\\": "src/nacos/"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests"
    }
  }
}